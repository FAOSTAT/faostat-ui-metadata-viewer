
{{#if addHeader}}
<br>
<p>{{metadata_welcome}}</p>
<h3>{{metadata}}</h3>
<br>
{{/if}}
{{#each data}}
    <h4 style="color:#337ab7;">{{this.code}}) {{this.label}}</h4>
    {{#each subsections}}
        <div class="row" style="margin-left:5px;font-size:13px;">
            <div class="col-md-3">
                <!--<p><small>{{this.code}}</small> {{this.label}}</p>-->
                <p><b>{{this.label}}</b></p>
            </div>
            <div class="col-md-9">
                <p>{{this.text}}</p>
            </div>
        </div>
    {{/each}}
    <hr>
{{/each}}